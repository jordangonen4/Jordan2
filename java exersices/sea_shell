function findSeashellsIndices(target, values) {
  for (let i = 0; i < values.length; i++) {
    for (let j = i + 1; j < values.length; j++) {
      if (values[i] + values[j] === target) {
        return [i, j];
      }
    }
  }
  return [];
}

console.log(findSeashellsIndices(30, [25, 21, 15, 10, 5])); 
console.log(findSeashellsIndices(40, [10, 20, 30, 5, 15])); 

